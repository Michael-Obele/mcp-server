{
  "name": "mcp-server",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "tsc",
    "start": "node build/index.js",
    "dev": "tsc --watch",
    "test": "set DANGEROUSLY_OMIT_AUTH=true && bunx @modelcontextprotocol/inspector bun run server:dev",
    "server:build": "tsc",
    "server:build:watch": "tsc --watch",
    "server:dev": "tsx src/index.ts",
    "server:inspect": "set DANGEROUSLY_OMIT_AUTH=true && npx @modelcontextprotocol/inspector npm run server:dev",
    "client:dev": "tsx src/client.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^24.3.1",
    "@types/turndown": "^5.0.5"
  },
  "dependencies": {
    "@modelcontextprotocol/inspector": "^0.16.7",
    "@modelcontextprotocol/sdk": "^1.17.5",
    "@mozilla/readability": "^0.6.0",
    "ai": "^4.3.16",
    "cheerio": "^1.1.2",
    "crawlee": "^3.14.1",
    "defuddle": "^0.6.6",
    "dotenv": "^16.5.0",
    "jsdom": "^27.0.0",
    "metascraper": "^5.49.2",
    "metascraper-description": "^5.49.2",
    "metascraper-title": "^5.49.2",
    "scrape-it": "^6.1.11",
    "tsx": "^4.20.5",
    "turndown": "^7.2.1",
    "website-scraper": "^5.3.1",
    "zod": "^3.25.67"
  }
}
